@using AdoptAnimal.Common
@model AdoptAnimal.Web.ViewModels.Articles.GetAllArticlesInputModel
@{
    this.ViewData["Title"] = "Get all articles";
}

<div class="text-center">
    @foreach (var article in Model.Articles)
   {
       <h3>@article.Title</h3>
       <p>@article.Content</p>
       <br />
   }
</div>